<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Version>1.0.1</Version>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>
  <PropertyGroup>
    <Summary>Fable bindings for Mocha.js</Summary>
    <Description>Provides fable bindings for the Mocha unit test framework.</Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Authors>Grant Neale</Authors>
    <PackageLicenseUrl>https://github.com/grantneale/fable-mocha/blob/master/LICENSE.md</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/grantneale/fable-mocha</PackageProjectUrl>
    <PackageTags>F#, fsharp, fable2, fable-bindings, mocha</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="Fable.Import.MochaJS.fs" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="*.fsproj; *.fs" PackagePath="fable\" />
  </ItemGroup>
  <Import Project="..\.paket\Paket.Restore.targets" />
</Project>
